4"},{"filePath":"82","messages":"83","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"/Users/darianlow/Projects/reactjs-practice/burger-builder/src/index.js",[],["84","85"],"/Users/darianlow/Projects/reactjs-practice/burger-builder/src/App.js",[],"/Users/darianlow/Projects/reactjs-practice/burger-builder/src/reportWebVitals.js",[],"/Users/darianlow/Projects/reactjs-practice/burger-builder/src/containers/BurgerBuilder/BurgerBuilder.js",["86"],"import Aux from '../../hoc/Aux/Aux';\nimport Burger from '../../components/Burger/Burger'\nimport BuildControls from '../../components/Burger/BuildControls/BuildControls';\nimport Modal from '../../components/UI/Modal/Modal';\nimport OrderSummary from '../../components/Burger/OrderSummary/OrderSummary';\nconst { Component } = require(\"react\");\n\nconst INGREDIENT_PRICES = {\n    salad: 0.5,\n    cheese: 0.4,\n    meat: 1.3,\n    bacon: 0.7\n}\n\nclass BurgerBuilder extends Component {\n\n    state = {\n        ingredients: {\n            salad: 0,\n            bacon: 0,\n            cheese: 0,\n            meat:0\n        },\n        totalPrice: 4,\n        purchaseable: false,\n        purchasing: false\n    }\n\n    updatePurchaseState (ingredients2) {\n        const ingredients = {\n            ...this.state.ingredients\n        };\n        const sum = Object.keys(ingredients2)\n            .map(igKey => {\n                return ingredients2[igKey]\n            })\n            .reduce((sum, el) => {\n                return sum + el;\n            }, 0);\n\n        this.setState({purchaseable: sum > 0})\n    }\n\n    addIngredientHanlder = (type) => {\n        const oldCount = this.state.ingredients[type];\n        const updatedCount = oldCount + 1;\n        const updatedIngredients = {\n            ...this.state.ingredients\n        };\n        updatedIngredients[type] = updatedCount;\n        const priceAddition = INGREDIENT_PRICES[type];\n        const oldPrice = this.state.totalPrice;\n        const newPrice = oldPrice + priceAddition;\n        this.setState({totalPrice: newPrice, ingredients: updatedIngredients});\n        this.updatePurchaseState(updatedIngredients);\n    }\n\n    removeIngredientHandler = (type) => {\n        \n        const oldCount = this.state.ingredients[type];\n        if (oldCount <= 0) {\n            return;\n        }\n        const updatedCount = oldCount - 1;\n        const updatedIngredients = {\n            ...this.state.ingredients\n        };\n        updatedIngredients[type] = updatedCount;\n        const priceDeduction = INGREDIENT_PRICES[type];\n        const oldPrice = this.state.totalPrice;\n        const newPrice = oldPrice - priceDeduction;\n        this.setState({totalPrice: newPrice, ingredients: updatedIngredients});\n        this.updatePurchaseState(updatedIngredients);\n    }\n    \n    purchaseHandler = () => {\n        this.setState( {purchasing: true})\n    }\n\n    purchaseCancelHandler = () => {\n        this.setState( {purchasing: false})\n    }\n\n    purchaseContinueHandler = () => {\n        alert('you\\'re continuing')\n    }\n    render() {\n        const disabledInfo = {\n            ...this.state.ingredients\n        };\n\n        for (let key in disabledInfo) {\n            disabledInfo[key] = disabledInfo[key] <= 0;\n        }\n\n        return (\n            <Aux>\n                <Modal \n                    show={this.state.purchasing}\n                    modalClosed={this.purchaseCancelHandler}\n                >\n                    <OrderSummary \n                        ingredients={this.state.ingredients}\n                        purchaseCanceled={this.purchaseCancelHandler}\n                        purchaseContinued={this.purchaseContinueHandler}\n                        price={this.state.totalPrice}\n                    />\n                </Modal>\n                <Burger ingredients={this.state.ingredients}/>\n                <BuildControls\n                    ingredientAdded={this.addIngredientHanlder} \n                    ingredientRemoved={this.removeIngredientHandler}\n                    disabled={disabledInfo}\n                    price={this.state.totalPrice}\n                    purchaseable={this.state.purchaseable}\n                    ordered={this.purchaseHandler}\n                />\n            </Aux>\n        );\n    }\n}\n\nexport default BurgerBuilder;","/Users/darianlow/Projects/reactjs-practice/burger-builder/src/components/Burger/Burger.js",[],"/Users/darianlow/Projects/reactjs-practice/burger-builder/src/components/Burger/BurgerIngredient/BurgerIngredient.js",[],"/Users/darianlow/Projects/reactjs-practice/burger-builder/src/components/Burger/BuildControls/BuildControls.js",[],"/Users/darianlow/Projects/reactjs-practice/burger-builder/src/components/Burger/BuildControls/BuildControl/BuildControl.js",[],"/Users/darianlow/Projects/reactjs-practice/burger-builder/src/components/UI/Modal/Modal.js",[],"/Users/darianlow/Projects/reactjs-practice/burger-builder/src/components/Burger/OrderSummary/OrderSummary.js",[],"/Users/darianlow/Projects/reactjs-practice/burger-builder/src/components/UI/Backdrop/Backdrop.js",[],"/Users/darianlow/Projects/reactjs-practice/burger-builder/src/components/UI/Button/Button.js",[],"/Users/darianlow/Projects/reactjs-practice/burger-builder/src/components/Navigation/Toolbar/Toolbar.js",["87"],"/Users/darianlow/Projects/reactjs-practice/burger-builder/src/components/Logo/Logo.js",[],"/Users/darianlow/Projects/reactjs-practice/burger-builder/src/components/Navigation/NavigationItems/NavigationItems.js",[],"/Users/darianlow/Projects/reactjs-practice/burger-builder/src/components/Navigation/NavigationItems/NavigationItem/NavigationItem.js",["88"],"/Users/darianlow/Projects/reactjs-practice/burger-builder/src/components/Navigation/SideDrawer/SideDrawer.js",[],"/Users/darianlow/Projects/reactjs-practice/burger-builder/src/components/Navigation/SideDrawer/DrawerToggle/DrawerToggle.js",[],"/Users/darian